<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
    <link rel="shortcut icon" href="favicon.ico">

    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,700,300' rel='stylesheet' type='text/css'>

    <!-- Animate.css -->
    <link rel="stylesheet" href="~/Content/layoutClient/css/animate.css">
    <!-- Icomoon Icon Fonts-->
    <link rel="stylesheet" href="~/Content/layoutClient/css/icomoon.css">
    <!-- Bootstrap  -->
    <link rel="stylesheet" href="~/Content/layoutClient/css/bootstrap.css">
    <!-- Superfish -->
    <link rel="stylesheet" href="~/Content/layoutClient/css/superfish.css">
    <!-- Magnific Popup -->
    <link rel="stylesheet" href="~/Content/layoutClient/css/magnific-popup.css">
    <!-- Date Picker -->
    <link rel="stylesheet" href="~/Content/layoutClient/css/bootstrap-datepicker.min.css">
    <!-- CS Select -->
    <link rel="stylesheet" href="~/Content/layoutClient/css/cs-select.css">
    <link rel="stylesheet" href="~/Content/layoutClient/css/cs-skin-border.css">
    <link rel="stylesheet" href="~/Content/layoutClient/css/style.css">
    <script src="~/Content/layoutClient/js/modernizr-2.6.2.min.js"></script>
</head>
<body>
    <div id="fh5co-wrapper">
        <div id="fh5co-page">

            <header id="fh5co-header-section" class="sticky-banner">
                <div class="container">
                    <div class="nav-header">
                        <a href="#" class="js-fh5co-nav-toggle fh5co-nav-toggle dark"><i></i></a>
                        <h1 id="fh5co-logo"><a href="index.html"><i class="icon-airplane"></i>Travel</a></h1>
                        <!-- START #fh5co-menu-wrap -->
                        <nav id="fh5co-menu-wrap" role="navigation">
                            <ul class="sf-menu" id="fh5co-primary-menu">
                                <li class="active"><a href="index.html">Home</a></li>
                                <li>
                                    <a href="" class="fh5co-sub-ddown">Train</a>
                                    <ul class="fh5co-sub-menu">
                                        <li><a href="#">Book Now</a></li>
                                        <li><a href="#">Train Schedule</a></li>
                                        <li><a href="#">Cancel ticket</a></li>
                                    </ul>
                                </li>
                                <li><a href="#">Fare</a></li>
                                @if (Session["Name"] != null)
                                {
                                    <li>

                                        <a href="" class="fh5co-sub-ddown">@Session["Name"].ToString()</a>
                                        <ul class="fh5co-sub-menu">
                                            <li><a href="@Url.Action("Logout", "Users")">Logout</a></li>
                                        </ul>

                                    </li>
                                }
                                else { 
                                    <li><a href="/Users/Login/">Login</a></li>
                                }

                            </ul>
                        </nav>
                    </div>
                </div>
            </header>

            <div>
                @RenderBody()
            </div>
            <footer>
                <div id="footer">
                    <div class="container">
                        <div class="row row-bottom-padded-md">
                            <div class="col-md-2 col-sm-2 col-xs-12 fh5co-footer-link">
                                <h3>About Travel</h3>
                                <p>Far far away, behind the word mountains, far from the countries Vokalia and Consonantia, there live the blind texts.</p>
                            </div>
                            <div class="col-md-2 col-sm-2 col-xs-12 fh5co-footer-link">
                                <h3>Top Flights Routes</h3>
                                <ul>
                                    <li><a href="#">Manila flights</a></li>
                                    <li><a href="#">Dubai flights</a></li>
                                    <li><a href="#">Bangkok flights</a></li>
                                    <li><a href="#">Tokyo Flight</a></li>
                                    <li><a href="#">New York Flights</a></li>
                                </ul>
                            </div>
                            <div class="col-md-2 col-sm-2 col-xs-12 fh5co-footer-link">
                                <h3>Top Hotels</h3>
                                <ul>
                                    <li><a href="#">Boracay Hotel</a></li>
                                    <li><a href="#">Dubai Hotel</a></li>
                                    <li><a href="#">Singapore Hotel</a></li>
                                    <li><a href="#">Manila Hotel</a></li>
                                </ul>
                            </div>
                            <div class="col-md-2 col-sm-2 col-xs-12 fh5co-footer-link">
                                <h3>Interest</h3>
                                <ul>
                                    <li><a href="#">Beaches</a></li>
                                    <li><a href="#">Family Travel</a></li>
                                    <li><a href="#">Budget Travel</a></li>
                                    <li><a href="#">Food &amp; Drink</a></li>
                                    <li><a href="#">Honeymoon and Romance</a></li>
                                </ul>
                            </div>
                            <div class="col-md-2 col-sm-2 col-xs-12 fh5co-footer-link">
                                <h3>Best Places</h3>
                                <ul>
                                    <li><a href="#">Boracay Beach</a></li>
                                    <li><a href="#">Dubai</a></li>
                                    <li><a href="#">Singapore</a></li>
                                    <li><a href="#">Hongkong</a></li>
                                </ul>
                            </div>
                            <div class="col-md-2 col-sm-2 col-xs-12 fh5co-footer-link">
                                <h3>Affordable</h3>
                                <ul>
                                    <li><a href="#">Food &amp; Drink</a></li>
                                    <li><a href="#">Fare Flights</a></li>
                                </ul>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6 col-md-offset-3 text-center">
                                <p class="fh5co-social-icons">
                                    <a href="#"><i class="icon-twitter2"></i></a>
                                    <a href="#"><i class="icon-facebook2"></i></a>
                                    <a href="#"><i class="icon-instagram"></i></a>
                                    <a href="#"><i class="icon-dribbble2"></i></a>
                                    <a href="#"><i class="icon-youtube"></i></a>
                                </p>
                                <p>Copyright 2016 Free Html5 <a href="#">Module</a>. All Rights Reserved. <br>Made with <i class="icon-heart3"></i> by <a href="http://freehtml5.co/" target="_blank">Freehtml5.co</a> / Demo Images: <a href="https://unsplash.com/" target="_blank">Unsplash</a></p>
                            </div>
                        </div>
                    </div>
                </div>
            </footer>



        </div>
        <!-- END fh5co-page -->
    </div>
    <script src="~/Content/layoutClient/js/jquery.min.js"></script>
    <!-- jQuery Easing -->
    <script src="~/Content/layoutClient/js/jquery.easing.1.3.js"></script>
    <!-- Bootstrap -->
    <script src="~/Content/layoutClient/js/bootstrap.min.js"></script>
    <!-- Waypoints -->
    <script src="~/Content/layoutClient/js/jquery.waypoints.min.js"></script>
    <script src="~/Content/layoutClient/js/sticky.js"></script>

    <!-- Stellar -->
    <script src="~/Content/layoutClient/js/jquery.stellar.min.js"></script>
    <!-- Superfish -->
    <script src="~/Content/layoutClient/js/hoverIntent.js"></script>
    <script src="~/Content/layoutClient/js/superfish.js"></script>
    <!-- Magnific Popup -->
    <script src="~/Content/layoutClient/js/jquery.magnific-popup.min.js"></script>
    <script src="~/Content/layoutClient/js/magnific-popup-options.js"></script>
    <!-- Date Picker -->
    <script src="~/Content/layoutClient/js/bootstrap-datepicker.min.js"></script>
    <!-- CS Select -->
    <script src="~/Content/layoutClient/js/classie.js"></script>
    <script src="~/Content/layoutClient/js/selectFx.js"></script>

    <!-- Main JS -->
    <script src="~/Content/layoutClient/js/main.js"></script>
</body>
</html>
